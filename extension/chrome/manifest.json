{
  "name": "Discord RichPresence for Chrome",
  "description": "This will enable some video services as RichPresence on your discord profile.",
  "icons": {
    "128": "assets/discord-logo.png"
  },
  "browser_action": {
    "default_icon": "assets/discord-logo.png"
  },
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/watch?v=*", "*://www.netflix.com/watch/*"],
      "js": ["RichPresence.js"]
    }
  ],
  "web_accessible_resources": [
    "youtube.js"
  ]
}
